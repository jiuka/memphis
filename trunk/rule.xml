<?xml version="1.0" encoding="UTF-8"?>
<rules version="0.1" autor="MariusRieder">
    <!-- Draw Background First -->
    <rule e="way" k="landuse|natural" v="*">
        <rule e="way" k="landuse|natural" v="forest|wood">
            <polygone color="0.3,0.8,0.3"/>
            <line color="0.2,0.7,0.2" width="1"/>
        </rule>
        <rule e="way" k="natural" v="water">
            <polygone color="0.3,0.3,0.8"/>
            <line color="0.2,0.2,0.7" width="1"/>
        </rule>
    </rule>
    <rule e="way" k="highway" v="*">
        <rule e="way" k="highway" v="track|footway">
            <line color="0.6,0.6,0.2" width="1"/>
        </rule>
        <rule e="way" k="highway" v="unclassified|residential">
            <line color="0.8,0.8,0.8" width="2"/>
            <line color="1.0,1.0,1.0" width="1"/>
        </rule>
        <rule e="way" k="highway" v="*">
            <line color="0.8,0.0,0.0" width="2"/>
            <line color="1.0,0.2,0.2" width="1"/>
        </rule>
    </rule>
</rules>